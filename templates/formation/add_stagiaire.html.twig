{% extends 'base.html.twig' %}

{% block title %}Ajouter un(des) stagiaire(s) à {{ session.intitule }}{% endblock %}

{% block body %}

<div id="wrapper-main">
    <h2>Choisir le(s) stagiaire(s) à ajouter</h2>
    {{ form_start(form) }}
    {{ form_widget(form.stagiaire) }}
    <br />
    {{ form_row(form.ajouter) }}
    {{ form_end(form) }}
</div>

{% endblock %}
