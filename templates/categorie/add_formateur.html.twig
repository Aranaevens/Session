{% extends 'base.html.twig' %}

{% block title %}Ajouter un(des) formateur(s) à {{ categorie.intitule }}{% endblock %}

{% block body %}
<main>
<div id="wrapper-main">
    <h2>Choisir le(s) formateurs(s) à ajouter</h2>
    {{ form_start(form) }}
    {{ form_widget(form.formateur) }}
    <br />
    {{ form_row(form.ajouter) }}
    {{ form_end(form) }}
</div>
</main>
{% endblock %}